{
    "version": 2,
    "builds": [
        {
            "src": "./index.py",
            "use": "@vercel/python"
        }
    ],
    "functions": {
        "index.py": {
            "memory": 1024,
            "maxDuration": 10,
            "runtime": "python3.9"
        }
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/index.py"
        }
    ]
}
